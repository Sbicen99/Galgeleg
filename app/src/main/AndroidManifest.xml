<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.galgeleg">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/Galgespillet"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:label"
        android:screenOrientation="portrait">
        <activity android:name=".Multiplayer.MultiplayerTabt"></activity>
        <activity android:name=".Multiplayer.MultiplayerVundet" />
        <activity android:name=".Multiplayer.SpilMedDinMakker_Spillet" />
        <activity android:name=".Multiplayer.SpilMedDinMakker"/>
        <activity android:name=".Hjælpeklasser.LoadingSide" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Hjælpeklasser.Oversigt" />
        <activity android:name=".Logik.Highscore_logik" />
        <activity android:name=".Singleplayer.GalgeSpillet" />
        <activity android:name=".SpilletsAfslutning.Tabt_galgespillet" />
        <activity android:name=".SpilletsAfslutning.Vundet_Galgespillet" />
        <activity android:name=".Hjælpeklasser.Hjaelp" />
        <activity android:name=".Singleplayer.Testrunde" />
        <activity android:name=".Logik.Logik" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>